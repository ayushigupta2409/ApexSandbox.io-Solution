public Boolean isPalindrome(String str){
//code here
    Set<String> digits = {'0','1','2','3','4','5',' ',','};
    for(String s: str){
        if(digits.contains(s)){
            str.remove(s);
        }
    }
    s=s.tolowercase();
    integer n = s.size();
    for(Integer i=0;i<n;i++){
        if(s[i] != s[n-i]){
            return False;
        }
    }
    return True;
}