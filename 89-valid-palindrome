public Boolean isPalindrome(String str){
//code here
    String regex = '[/\\W]';
    str = str.replaceAll(regex, '').toLowerCase();Integer n = str.length();
    str=str.tolowercase();
    n = str.length();
    for(Integer i=0;i<n;i++){
        if(str.charAt(i) != str.charAt(n-i)){
            return False;
        }
    }
    return True;
}