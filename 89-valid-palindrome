public Boolean isPalindrome(String str){
//code here
    String digits = ', ';
    for(String s: str){
        if(digits.contains(s)){
            str.replace(s,'');
        }
    }
    s=s.tolowercase();
    integer n = s.size();
    for(Integer i=0;i<n;i++){
        if(s.charAt(i) != s.charAt(n-i)){
            return False;
        }
    }
    return True;
}