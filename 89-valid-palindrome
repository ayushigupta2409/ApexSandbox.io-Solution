public Boolean isPalindrome(String str){
//code here
    String digits = ', ';
    Integer n = str.length();
    for(Integer i=0;i<n;i++){
        if(digits.contains(str.charAt(i))){
            str.replace(str.charAt(i),'');
        }
    }
    str=str.tolowercase();
    n = str.length();
    for(Integer i=0;i<n;i++){
        if(str.charAt(i) != str.charAt(n-i)){
            return False;
        }
    }
    return True;
}