public Boolean isPalindrome(String str){
//code here
    String digits = ', ';
    for(String s: str){
        if(digits.contains(s)){
            str.replace(s,'');
        }
    }
    str=str.tolowercase();
    integer n = str.size();
    for(Integer i=0;i<n;i++){
        if(str.charAt(i) != str.charAt(n-i)){
            return False;
        }
    }
    return True;
}