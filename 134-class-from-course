public Id classFromCourse(String courseName) {
    // code here
    apxio__Class__c c = new apxio__Class__c();
    apxio__Course__c h = [select Id,Name from apxio__Course__c where Name = courseName];
    if (h != null){
        c.apxio__Course__c = h.Id;
        insert c;
        return c.Id;
    }
    return null;
}