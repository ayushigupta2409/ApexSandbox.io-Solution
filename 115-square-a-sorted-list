public static List<Integer> squareList(List<Integer> nums){
    //solution here
    List<Integer> n = new List<Integer>();
    for(Integer i=0;i<nums.size();i++){
        n.add(nums[i]*nums[i]);
    }
    //n.sort();
    for(Integer i=0;i<n.size()-1;i++){
        if(Integer j=i+1;i<n.size();j++){
            if(n[i]<n[j]) continue;
            else{
                Integer temp=n[i];
                n[i]=n[j];
                n[j]=n[i];
            }
        }
    }
    return n;
}