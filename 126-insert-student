public Id insertStudent(String name, String email) {
    // code here
    apxio__Student__c s = new apxio__Student__c();
    s.Name = name;
    s.apxio__Email__c = email;
    insert s;
    apxio__Student__c a = [Select Id,Name,apxio__Email__c from apxio__Student__c where Name==name and apxio__Email__c==email];
    return a.Id;
}